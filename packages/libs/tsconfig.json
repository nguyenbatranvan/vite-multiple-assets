{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"target": "es5",
		"lib": ["es6", "ESNext"],
		"module": "commonjs",
		"rootDir": ".",
		"resolveJsonModule": true,
		"allowJs": true,
		"outDir": "src/build",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"noImplicitAny": false,
		"skipLibCheck": true
	}
}
