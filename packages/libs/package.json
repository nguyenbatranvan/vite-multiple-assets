{
  "name": "vite-multiple-assets",
  "version": "2.2.0",
  "description": "Multiple public asset directories for vite",
  "author": "Van nguyen ba tran",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/nguyenbatranvan/vite-multiple-assets"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "module": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "scripts": {
    "build-npm": "rm -rf dist && tsup src/index.ts --dts --format cjs,esm",
    "build-npm-win32": "rmdir /S /Q dist && tsup src/index.ts --dts --format cjs,esm",
    "build": "npm run build-npm",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vite-multiple-assets",
    "Vite",
    "Multiple Assets",
    "Multiple directory"
  ],
  "devDependencies": {
    "@swc/core": "^1.7.40",
    "@types/micromatch": "^4.0.9",
    "@types/mime-types": "^2.1.4",
    "@types/watchpack": "^2.4.4",
    "@types/node": "^22.8.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "peerDependencies": {
    "mime-types": "^2.1.35",
    "vite": ">=2.9.6"
  },
  "dependencies": {
    "watchpack": "^2.4.2",
    "fast-glob": "^3.3.2",
    "micromatch": "^4.0.7"
  },
  "gitHead": "1f7378ff8a5bc22252de6f3e9b1f38bee078a663"
}
