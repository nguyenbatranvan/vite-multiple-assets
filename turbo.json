{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "lint": {},
    "format": {},
    "reformat": {},
    "build-npm": {},
    "local": {},
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "persistent": true,
      "cache": false
    },
    "publish": {},
    "e2e": {}
  }
}
